{"ast":null,"code":"import axios from 'axios';\nimport { ref, onMounted } from 'vue';\nimport * as echarts from 'echarts';\nexport default {\n  __name: 'hobbiesPage',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    let hobbyData = ref({\n      span: [],\n      hobby: [],\n      corsswideWork: [],\n      verticalWork: []\n    });\n    onMounted(() => {\n      axios.get('/data/resumeData/chinese/hobbies.json').then(response => {\n        hobbyData.value = response.data;\n        // 准备echarts数据\n        const chartData = hobbyData.value.span.map(item => ({\n          value: Number(item.percent),\n          name: item.title\n        }));\n        var chartDom = document.getElementById('chartsShow');\n        var myChart = echarts.init(chartDom);\n        var option;\n        option = {\n          textStyle: {\n            fontSize: 16,\n            // 设置全局字体大小\n            fontWeight: 600\n          },\n          tooltip: {\n            trigger: 'item',\n            formatter: function (params) {\n              const total = params.data.value; // 当前项的值\n              const percent = (total / chartData.reduce((sum, item) => sum + item.value, 0) * 100).toFixed(0); // 计算百分比\n              return `${params.name}: ${percent}%`; // 显示名称和百分比\n            }\n          },\n          series: [{\n            name: 'Hobby From',\n            type: 'pie',\n            radius: ['20%', '70%'],\n            itemStyle: {\n              borderRadius: 10,\n              borderColor: '#fff',\n              borderWidth: 2\n            },\n            emphasis: {\n              label: {\n                show: true,\n                fontSize: 35,\n                fontWeight: 'bold'\n              }\n            },\n            labelLine: {\n              show: false\n            },\n            data: chartData\n          }]\n        };\n        option && myChart.setOption(option);\n      }).catch(error => {\n        console.log(\"加载兴趣爱好数据出错\", error);\n      });\n    });\n    const __returned__ = {\n      get hobbyData() {\n        return hobbyData;\n      },\n      set hobbyData(v) {\n        hobbyData = v;\n      },\n      get axios() {\n        return axios;\n      },\n      ref,\n      onMounted,\n      get echarts() {\n        return echarts;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["axios","ref","onMounted","echarts","hobbyData","span","hobby","corsswideWork","verticalWork","get","then","response","value","data","chartData","map","item","Number","percent","name","title","chartDom","document","getElementById","myChart","init","option","textStyle","fontSize","fontWeight","tooltip","trigger","formatter","params","total","reduce","sum","toFixed","series","type","radius","itemStyle","borderRadius","borderColor","borderWidth","emphasis","label","show","labelLine","setOption","catch","error","console","log"],"sources":["D:/gitRepository/resume/src/components/sessionDeatil/hobbiesPage.vue"],"sourcesContent":["<template>\n    <div class=\"hobbies w-100 bg-light border-bottom border-dark\">\n        <div class=\"page d-flex flex-column align-items-start\">\n            <h1 class=\"title display-5 mb-4 \">\n                <span class=\"text-color \">兴趣</span><span class=\"fw-bold\">爱好</span>\n            </h1>\n            <hr class=\"solid-divider ms-1 me-1 mb-3 mb-lg-4\">\n            <p class=\"intro h5 text-start text-muted\">我对于空闲时间的大概花费</p>\n\n            <!-- 图标区域 -->\n            <div class=\"chartsContainer my-2 mb-5\">\n                <div id=\"chartsShow\" style=\"width: 700px;height:400px;\"></div>\n\n                <!-- 闲时花销内容 -->\n                <div class=\"hobby d-flex\" v-for=\"(span, i) in hobbyData.span\" :key=\"i\">\n                    <div class=\"iocn \" :style=\"{ backgroundColor: span.bgColor }\">\n                        <i data-v-a8356e5e=\"\" :class=span.iconClass></i>\n                    </div>\n                    <div class=\"content d-flex flex-column\">\n                        <div class=\"title fw-bold text-start\">{{ span.title }}</div>\n                        <div class=\"detail text-muted text-start\">{{ span.content }}</div>\n                    </div>\n                </div>\n            </div>\n\n            <!-- 兴趣区域 -->\n            <h3 class=\"title m-2\">\n                <span class=\"r fw-bold\">兴趣 </span>\n                <i class=\"fa fa-book-open-reader ms-2\"></i>\n                <hr class=\"solid-divider w-100 \">\n            </h3>\n            <div class=\"hobby_item d-flex m-2 my-2\" v-for=\"(hobby, i) in hobbyData.hobby\" :key=\"i\">\n                <div class=\"iocn mx-3\">\n                    <i data-v-a8356e5e=\"\" :class=hobby.iconClass></i>\n                </div>\n                <div class=\"content d-flex flex-column\">\n                    <div class=\"title fw-bold text-start\">{{ hobby.name }}</div>\n                    <div class=\"detail text-muted text-start\">{{ hobby.content }}</div>\n                </div>\n            </div>\n\n            <!-- 兴趣作品 -->\n            <h3 class=\"title m-2 mt-5 \">\n                <span class=\"r fw-bold\">兴趣作品 </span>\n                <i class=\"fas fa-camera\"></i>\n                <hr class=\"solid-divider w-100 \">\n            </h3>\n\n            <div class=\"works d-flex\">\n                <div class=\"crosswise\">\n                    <el-carousel :interval=\"3000\" indicator-position=\"outside\" height=\"34vw\" >\n                        <el-carousel-item v-for=\"(work,index) in hobbyData.corsswideWork\" :key=\"index\">\n                            <div>\n                                <img :src=work.ImgUrl alt=\"\">\n                            </div>\n                        </el-carousel-item>\n                    </el-carousel>\n                </div>\n                <div class=\"vertical\">\n                    <el-carousel :interval=\"3000\" indicator-position=\"outside\" height=\"40vw\">\n                        <el-carousel-item v-for=\"(work,index) in hobbyData.verticalWork\" :key=\"index\">\n                            <div>\n                                <img :src=work.ImgUrl alt=\"\">\n                            </div>\n                        </el-carousel-item>\n                    </el-carousel>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n\n<script setup>\nimport axios from 'axios';\nimport { ref, onMounted } from 'vue';\nimport * as echarts from 'echarts';\n\nlet hobbyData = ref({\n    span: [],\n    hobby: [],\n    corsswideWork:[],\n    verticalWork:[],\n});\n\n\nonMounted(() => {\n    axios.get('/data/resumeData/chinese/hobbies.json')\n        .then(response => {\n            hobbyData.value = response.data;\n            // 准备echarts数据\n            const chartData = hobbyData.value.span.map(item => ({\n                value: Number(item.percent),\n                name: item.title\n            }));\n\n\n            var chartDom = document.getElementById('chartsShow');\n            var myChart = echarts.init(chartDom);\n            var option;\n            option = {\n\n                textStyle: {\n                    fontSize: 16, // 设置全局字体大小\n                    fontWeight: 600\n                },\n                tooltip: {\n                    trigger: 'item',\n                    formatter: function (params) {\n                        const total = params.data.value; // 当前项的值\n                        const percent = ((total / chartData.reduce((sum, item) => sum + item.value, 0)) * 100).toFixed(0); // 计算百分比\n                        return `${params.name}: ${percent}%`; // 显示名称和百分比\n                    }\n                },\n                series: [\n                    {\n                        name: 'Hobby From',\n                        type: 'pie',\n                        radius: ['20%', '70%'],\n\n                        itemStyle: {\n                            borderRadius: 10,\n                            borderColor: '#fff',\n                            borderWidth: 2\n                        },\n                        emphasis: {\n                            label: {\n                                show: true,\n                                fontSize: 35,\n                                fontWeight: 'bold'\n                            },\n                        },\n                        labelLine: {\n                            show: false\n                        },\n                        data: chartData\n                    }\n                ]\n            };\n            option && myChart.setOption(option);\n\n        })\n        .catch(error => {\n            console.log(\"加载兴趣爱好数据出错\", error);\n        });\n});\n\n</script>\n\n<style scoped>\n.hobbies {\n    .page {\n        margin: 30px 30px;\n        padding: 60px;\n\n        .text-color {\n            color: blueviolet;\n            font-weight: 600;\n        }\n\n        .solid-divider {\n            border: 0;\n            width: 20%;\n            height: 4px;\n            background-color: blueviolet;\n            /* 设置线条的颜色 */\n            margin-left: 0.25rem;\n        }\n\n        .chartsContainer {\n            display: flex;\n            flex-wrap: wrap;\n            gap: 20px;\n\n            .hobby {\n                flex-basis: calc(50% - 20px);\n                /* 每个组件占一行的 50% 宽度 */\n                display: flex;\n                padding: 0 2%;\n\n                .iocn {\n                    width: 50px;\n                    height: 50px;\n                    flex-shrink: 0;\n                    /* 防止缩小 */\n                    display: flex;\n                    justify-content: center;\n                    align-items: center;\n                    font-size: 28px;\n                    border-radius: 50%;\n                }\n\n                .content {\n                    padding: 0 4%\n                }\n            }\n        }\n\n        .hobby_item {\n            .iocn {\n                width: 50px;\n                height: 50px;\n                flex-shrink: 0;\n                /* 防止缩小 */\n                display: flex;\n                justify-content: center;\n                align-items: center;\n                font-size: 28px;\n                border-radius: 50%;\n                background-color: blueviolet;\n\n            }\n\n        }\n\n        .works {\n            width: 100%;\n            height: 680px;\n            justify-content: space-between;\n            gap: 20px;\n            .crosswise {\n                width: 60%;\n                height: 500px;\n                aspect-ratio: 16 / 9;\n                img{\n                    width: 100%;\n                }\n            }\n            .vertical{\n                width: 35%;\n                aspect-ratio: 16 / 9;\n                flex:1;\n                img{\n                    width: 100%;\n                    height: auto;\n                }\n            }\n        }\n    }\n}\n</style>"],"mappings":"AA0EA,OAAOA,KAAK,MAAM,OAAO;AACzB,SAASC,GAAG,EAAEC,SAAS,QAAQ,KAAK;AACpC,OAAO,KAAKC,OAAO,MAAM,SAAS;;;;;;;IAElC,IAAIC,SAAS,GAAGH,GAAG,CAAC;MAChBI,IAAI,EAAE,EAAE;MACRC,KAAK,EAAE,EAAE;MACTC,aAAa,EAAC,EAAE;MAChBC,YAAY,EAAC;IACjB,CAAC,CAAC;IAGFN,SAAS,CAAC,MAAM;MACZF,KAAK,CAACS,GAAG,CAAC,uCAAuC,CAAC,CAC7CC,IAAI,CAACC,QAAQ,IAAI;QACdP,SAAS,CAACQ,KAAK,GAAGD,QAAQ,CAACE,IAAI;QAC/B;QACA,MAAMC,SAAS,GAAGV,SAAS,CAACQ,KAAK,CAACP,IAAI,CAACU,GAAG,CAACC,IAAI,KAAK;UAChDJ,KAAK,EAAEK,MAAM,CAACD,IAAI,CAACE,OAAO,CAAC;UAC3BC,IAAI,EAAEH,IAAI,CAACI;QACf,CAAC,CAAC,CAAC;QAGH,IAAIC,QAAQ,GAAGC,QAAQ,CAACC,cAAc,CAAC,YAAY,CAAC;QACpD,IAAIC,OAAO,GAAGrB,OAAO,CAACsB,IAAI,CAACJ,QAAQ,CAAC;QACpC,IAAIK,MAAM;QACVA,MAAM,GAAG;UAELC,SAAS,EAAE;YACPC,QAAQ,EAAE,EAAE;YAAE;YACdC,UAAU,EAAE;UAChB,CAAC;UACDC,OAAO,EAAE;YACLC,OAAO,EAAE,MAAM;YACfC,SAAS,EAAE,SAAAA,CAAUC,MAAM,EAAE;cACzB,MAAMC,KAAK,GAAGD,MAAM,CAACpB,IAAI,